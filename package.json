        "cytoscape": "3.25.0",
        "cytoscape-cola": "2.2.3",
        "cytoscape-context-menus": "4.1.0",
        "cytoscape-layout-utilities": "1.1.1",
        "cytoscape-node-html-label": "1.2.2",
        "cytoscape-popper": "2.0.0",
        "cytoscape-undo-redo": "1.3.3",
        "d3": "^7.8.5",
        "dotenv": "^14.2.0",
        "express": "^4.17.1",
        "express-fileupload": "^1.2.1",
        "extract-zip-relative-path": "^2.0.4",
        "fs": "0.0.1-security",
        "hack-font": "^3.3.0",
        "material-icons": "^1.13.9",
        "materialize-css": "^1.0.0-rc.2",
        "move-file-cli": "^3.0.0",
        "parse-json": "^5.2.0",
        "pure-context-menu": "^1.1.1",
        "recursive-fs": "^2.1.0",
        "socket.io": "^4.7.2",
